    ViewBag.Model = new LibVisWeb.Models.ArticleModel() { Title = "Teste", Paragraphs = new List<string>(new[] { "paragrafo 1", "paragrafo 2", "paragrafo 3" })};
}
<div id="react-app" asp-prerender-module="ClientApp/dist/main-server.js" 
